    <validationRules>
        <fullName>Validation_Rule_on_Permission</fullName>
        <active>true</active>
        <description>Permission Related to RCSPO.</description>
        <errorConditionFormula>AND( $User.Id != &quot;005d0000001WLvx&quot;, /*Jane McInerney*/ 
$User.Id != &quot;0050V000006jKNy&quot;, /*Nyle Grimes*/ 
$User.Id != &quot;005d0000001Wyup&quot;, /*Kara Desmarais*/ 
$User.Id != &quot;005d0000001Vmik&quot;, /*Wendy Firdman*/ 
$User.Id != &quot;005d0000000Tvsu&quot;, /*LaJuanya Jones*/ 
$User.Id != &quot;005d0000001VstM&quot;, /*Rose Albert*/
$User.Id != &quot;005d0000000Twtn&quot;, /*Joseph Kensil*/ 
$User.Id != &quot;005d0000002mPMz&quot;, /*Stephen Lynch*/  
$Profile.Name != &quot;System Administrator&quot;, /*System Admin*/ 
OR( 
AND(ISNEW(), OR(INCLUDES(PHSS_Channel__c, &quot;RCS Retail&quot;), INCLUDES(PHSS_Channel__c, &quot;RCS Distributor&quot;))), /*Restriction in RCSPO Account creating*/ 
OR(INCLUDES(PHSS_Channel__c, &quot;RCS Retail&quot;), INCLUDES(PHSS_Channel__c, &quot;RCS Distributor&quot;)), /*Restriction in Changing RCSPO Account*/ 
OR(INCLUDES(PRIORVALUE(PHSS_Channel__c), &quot;RCS Retail&quot;), INCLUDES(PRIORVALUE(PHSS_Channel__c), &quot;RCS Distributor&quot;))) /*Change in RCSPO Account to other Account*/ 
)</errorConditionFormula>
        <errorMessage>You do not have permission to Create/Edit RCS PO Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_CFS_Account_number</fullName>
        <active>false</active>
        <description>CFS Account Number must read PRXXXXXXXX (PR plus 8 numeric characters)</description>
        <errorConditionFormula>AND(OR((MID(CFS_Account_Number_1__c,0,2) &lt;&gt; &apos;PR&apos;), NOT(ISNUMBER(MID(CFS_Account_Number_1__c,3,LEN(CFS_Account_Number_1__c)))), LEN(CFS_Account_Number_1__c) &lt;&gt; 9, CONTAINS(CFS_Account_Number_1__c,&apos;.&apos;)), !ISBLANK(CFS_Account_Number_1__c))</errorConditionFormula>
        <errorDisplayField>CFS_Account_Number_1__c</errorDisplayField>
        <errorMessage>CFS Account Number must of type PRXXXXXXXX</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_rule_on_Red_cross_Email</fullName>
        <active>false</active>
        <description>Permission on Red cross Email log in field.</description>
        <errorConditionFormula>AND( $User.Id != &quot;005d0000001WLvx&quot;, /*Jane McInerney*/ 
$User.Id != &quot;0050n000000reUm&quot;, /*Nyle Grimes*/ 
$Profile.Name != &quot;System Administrator&quot;, /*System Admin*/ 
OR(
AND(!ISNEW(), ISCHANGED(Log_In_Email__c)), /*Change in field*/ 
AND(ISNEW(), !ISBLANK(Log_In_Email__c)))/*New record*/ 
)</errorConditionFormula>
        <errorDisplayField>Log_In_Email__c</errorDisplayField>
        <errorMessage>You do not have permission to modify this field.</errorMessage>
    </validationRules>
