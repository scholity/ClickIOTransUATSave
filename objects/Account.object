    <validationRules>
        <fullName>Account_Number_Length</fullName>
        <active>true</active>
        <description>Validates that the Account Number is exactly eight digits (if it is not blank). The
number eight is simply illustrative. You can change this to any number you like.</description>
        <errorConditionFormula>AND(
LEN(AccountNumber) &gt; 0,
LEN(AccountNumber) &lt;&gt; 8, $Setup.Validation_Rules__c.Bypass_Rules__c = false

)</errorConditionFormula>
        <errorDisplayField>AccountNumber</errorDisplayField>
        <errorMessage>Account Number must be eight digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Number_is_Numeric</fullName>
        <active>true</active>
        <description>Validates that the Account Number is numeric if not blank</description>
        <errorConditionFormula>AND(
LEN ( AccountNumber) &gt; 0,
NOT (ISNUMBER( AccountNumber )),
 $Setup.Validation_Rules__c.Bypass_Rules__c = false
)</errorConditionFormula>
        <errorDisplayField>AccountNumber</errorDisplayField>
        <errorMessage>Account Number is not numeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Revenue_Range</fullName>
        <active>true</active>
        <description>Validates that the account Annual Revenue is not negative and also does not exceed $100 billion. This limit is designed to catch typos.You can definitely define your own limit for this</description>
        <errorConditionFormula>AND(
OR(
AnnualRevenue &lt; 0,
AnnualRevenue &gt; 100000000000

),
 $Setup.Validation_Rules__c.Bypass_Rules__c = false)</errorConditionFormula>
        <errorDisplayField>AnnualRevenue</errorDisplayField>
        <errorMessage>Annual Revenue cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bankruptcy_Date_Approval</fullName>
        <active>true</active>
        <description>This VR defines that only specific users are able edit Bankruptcy Date field</description>
        <errorConditionFormula>And(
NOT(ISBLANK(Bankruptcy_Date__c)),
AND
(ISCHANGED(Bankruptcy_Date__c)),

AND(
$User.Id != &quot;005d0000001WGso&quot;, /*Anne Hathaway*/
$User.Id != &quot;005d0000001WGtI&quot;, /*Kwesida Rhoney*/
$User.Id != &quot;005d0000001Vmik&quot;, /*Wendy Firdman*/
$User.Id != &quot;0050V000006lVNt&quot;, /*Craig Terrell*/
$User.Id != &quot;005d0000001Wyup&quot;, /*Kara Desmaris*/
$User.Id != &quot;0050V000006jKNy&quot;, /*Nyle Grimes*/
$User.Id != &quot;005d0000001WGrq&quot;, /*Mandy Skomorucha*/
$User.Id != &quot;005d00000040uDS&quot;, /*Gita Borovsky*/
$User.Id != &quot;005d0000005I9dx&quot; /*Mike A Smith*/
),

$Setup.Validation_Rules__c.Bypass_Rules__c = false )</errorConditionFormula>
        <errorDisplayField>Bankruptcy_Date__c</errorDisplayField>
        <errorMessage>You don&apos;t have permission to set the Bankruptcy Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bankruptcy_Type_Approval</fullName>
        <active>false</active>
        <description>This VR defines that only specific users are able edit Bankruptcy Type field.</description>
        <errorConditionFormula>AND(
AND(
ISCHANGED(Bankruptcy_Type__c),
OR(
ISPICKVAL(Bankruptcy_Type__c, &quot;Chapter 7 (out of business)&quot;),
ISPICKVAL(Bankruptcy_Type__c, &quot;Chapter 9 (municipal)&quot;),
ISPICKVAL(Bankruptcy_Type__c, &quot;Chapter 11 (restructure)&quot;))),

AND(
$User.Id != &quot;005d0000001WGso&quot;, /*Anne Hathaway*/
$User.Id != &quot;005d0000001WGtI&quot;, /*Kwesida Rhoney*/
$User.Id != &quot;005d0000001Vmik&quot;, /*Wendy Firdman*/
$User.Id != &quot;0050V000006lVNt&quot;, /*Craig Terrell*/
$User.Id != &quot;005d0000001Wyup&quot;, /*Kara Desmaris*/
$User.Id != &quot;0050V000006jKNy&quot;, /*Nyle Grimes*/
$User.Id != &quot;005d0000001WGrq&quot;, /*Mandy Skomorucha*/
$User.Id != &quot;005d00000040uDS&quot;, /*Gita Borovsky*/
$User.Id != &quot;005d0000005I9dx&quot; /*Mike A Smith*/
),

$Setup.Validation_Rules__c.Bypass_Rules__c = False)</errorConditionFormula>
        <errorDisplayField>Bankruptcy_Type__c</errorDisplayField>
        <errorMessage>You do not have permission to make this change. Contact Customer Ops if you are receiving this message in error.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Contact_Required_for_Invoicing</fullName>
        <active>false</active>
        <description>The Billing Contact is required for Invoicing Accounts. Invoicing Accounts are determined by the Preferred Payment Type=Invoice.</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Preferred_Payment_type__c, &apos;Invoice&apos;), ISBLANK(Billing_Contact__c ) ) , TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Billing Contact must be listed on the Account when the Preferred Payment Type equals Invoicing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_RCS_channel_Billing_Contacts</fullName>
        <active>false</active>
        <description>Checks if the Billing contact type is &quot;RCS Billing&quot; for the PHSS channel RCS Retail or RCS Distributor</description>
        <errorConditionFormula>AND(
        !ISBLANK(Billing_Contact__c),
       NOT(AND( 
        OR( 
          INCLUDES(PHSS_Channel__c, &quot;RCS Retail&quot;), 
          INCLUDES(PHSS_Channel__c, &quot;RCS Distributor&quot;)
         ),
 
    INCLUDES(Billing_Contact__r.Contact_Type__c,&quot;RCS Billing&quot;)
         )   )

)</errorConditionFormula>
        <errorDisplayField>Billing_Contact__c</errorDisplayField>
        <errorMessage>RCS Billing Contact Type has to be RCS Billing for RCS Retail or RCS Distributors channels</errorMessage>
    </validationRules>
